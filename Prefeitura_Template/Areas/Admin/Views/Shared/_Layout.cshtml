    <script>
        function exportar() {
            var values = new Array();

            $('.inters').each(function () {
                values.push($(this).val());
            });

            if (values.length > 0) {
                var action = '@Url.Action("ExportExc")';
                var postFormStr = "<form method='POST' action='" + action + "'>\n";

                for (var key in values) {
                    if (values.hasOwnProperty(key)) {
                        postFormStr += "<input type='hidden' name='inter[" + key + "]' value='" + values[key] + "'></input>";
                    }
                }

                postFormStr += "</form>";

                var formElement = $(postFormStr);

                $('body').append(formElement);
                $(formElement).submit();
            }
        };
    </script>
